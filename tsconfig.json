{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */
    "target": "es6",
    "module": "esNext",
    "moduleResolution": "node",
    "importHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "sourceMap": true,
    "declaration": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "es2015.generator",
      "es2015.symbol.wellknown",
      "es2020.bigint"
    ],
    "rootDir": "src" /* Disallow inconsistently-cased references to the same file. */
  }
}